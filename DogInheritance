#include <iostream>
using namespace std;

class Animal
{
	public:
		void eat()
		{
			cout<< "It does eat" <<endl;
		}
		void sleep()
		{
			cout<< "It does sleep" <<endl;
		}
};

class Dog:public Animal
{
	public:
		void bark()
		{
			cout<< "It barks loud" <<endl;
		}
};

int main()
{
	Dog d1;
	
	d1.eat();
	d1.sleep();
	d1.bark();
}
